function functionitem:AddLabelX(options)
	local text = options.Name
	local Flag = options.Flag
	local Side = options.Side or "Left"

	local textas = {}
	local Label = Instance.new("Frame")
	local Text = Instance.new("TextLabel")
	Label.Name = text
	Label.Parent = SectionContainer
	Label.BackgroundTransparency = 1.000
	Label.Size = UDim2.new(0, 265, 0, 30)

	if Side == "Left" then
		Label.AnchorPoint = Vector2.new(0, 0.5)
		Label.Position = UDim2.new(0, 10, 0, 0)
	else
		Label.AnchorPoint = Vector2.new(0.5, 0.5)
		Label.Position = UDim2.new(0.5, 0, 0, 0)
	end

	Text.Name = "Text"
	Text.Parent = Label
	Text.AnchorPoint = Vector2.new(0.5, 0.5)
	Text.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
	Text.BackgroundTransparency = 1.000
	Text.Position = UDim2.new(0.5, 0, 0.5, 0)
	Text.Size = UDim2.new(1, 0, 1, 0)
	Text.ZIndex = 16
	Text.Font = Enum.Font.GothamBold
	Text.Text = text
	Text.TextColor3 = Color3.fromRGB(255, 255, 255)
	Text.TextSize = 14.000
	Text.TextXAlignment = Enum.TextXAlignment.Left

	function textas:Set(newtext)
		Text.Text = newtext
	end

	return textas
end
function functionitem:AddLabelXX(options)
	local text = options.Name
	local Flag = options.Flag
	local Side = options.Side or "Left"
	local IconId = options.Icon

	local textas = {}
	local Label = Instance.new("Frame")
	Label.Name = text
	Label.Parent = SectionContainer
	Label.BackgroundTransparency = 1
	Label.Size = UDim2.new(0, 265, 0, 30)

	if Side == "Left" then
		Label.AnchorPoint = Vector2.new(0, 0.5)
		Label.Position = UDim2.new(0, 10, 0, 0)
	else
		Label.AnchorPoint = Vector2.new(0.5, 0.5)
		Label.Position = UDim2.new(0.5, 0, 0, 0)
	end

	local IconSize = 20
	if IconId then
		local Icon = Instance.new("ImageLabel")
		Icon.Name = "Icon"
		Icon.Parent = Label
		Icon.BackgroundTransparency = 1
		Icon.Position = UDim2.new(0, 5, 0.5, -IconSize/2)
		Icon.Size = UDim2.new(0, IconSize, 0, IconSize)
		Icon.Image = "rbxassetid://" .. tostring(IconId)
		Icon.ZIndex = 0

		local IconShadow = Instance.new("ImageLabel")
		IconShadow.Name = "IconShadow"
		IconShadow.Parent = Label
		IconShadow.BackgroundTransparency = 1
		IconShadow.Position = Icon.Position + UDim2.new(0, 1, 0, 1)
		IconShadow.Size = Icon.Size
		IconShadow.Image = Icon.Image
		IconShadow.ImageColor3 = Color3.fromRGB(0, 0, 0)
		IconShadow.ImageTransparency = 0.7
		IconShadow.ZIndex = 16
	end
	local Text = Instance.new("TextLabel")
	Text.Name = "Text"
	Text.Parent = Label
	Text.AnchorPoint = Vector2.new(0, 0.5)
	Text.BackgroundTransparency = 1
	Text.Position = IconId and UDim2.new(0, IconSize + 10, 0.5, 0) or UDim2.new(0, 5, 0.5, 0)
	Text.Size = UDim2.new(1, -IconSize - 15, 1, 0)
	Text.ZIndex = 18
	Text.Font = Enum.Font.GothamBold
	Text.Text = text
	Text.TextColor3 = Color3.fromRGB(255, 255, 255)
	Text.TextSize = 14
	Text.TextXAlignment = Enum.TextXAlignment.Left

	function textas:Set(newtext)
		Text.Text = newtext
	end

	return textas
end

	
