				function functionitem:AddLabel(options)
	local parts = options.NameParts or { {Text = options.Name or "Label", Color = Color3.fromRGB(255, 255, 255)} }

	local textas = {}

	local Label = Instance.new("Frame")
	Label.Name = "MultiColorLabel"
	Label.Parent = SectionContainer
	Label.AnchorPoint = Vector2.new(0.5, 0.5)
	Label.BackgroundTransparency = 1
	Label.Size = UDim2.new(0, 265, 0, 140)

	local layout = Instance.new("UIListLayout")
	layout.FillDirection = Enum.FillDirection.Horizontal
	layout.HorizontalAlignment = Enum.HorizontalAlignment.Left
	layout.SortOrder = Enum.SortOrder.LayoutOrder
	layout.Parent = Label

	for _, part in ipairs(parts) do
		local Text = Instance.new("TextLabel")
		Text.Name = "TextPart"
		Text.Parent = Label
		Text.BackgroundTransparency = 1
		Text.Size = UDim2.new(0, 0, 0, 20)
		Text.AutomaticSize = Enum.AutomaticSize.X
		Text.Font = Enum.Font.GothamBold
		Text.Text = part.Text
		Text.TextColor3 = part.Color
		Text.TextSize = 14
		Text.TextXAlignment = Enum.TextXAlignment.Left
	end

	function textas:Set(partsTable)
		for _, child in ipairs(Label:GetChildren()) do
			if child:IsA("TextLabel") then
				child:Destroy()
			end
		end

		for _, part in ipairs(partsTable) do
			local Text = Instance.new("TextLabel")
			Text.Name = "TextPart"
			Text.Parent = Label
			Text.BackgroundTransparency = 1
			Text.Size = UDim2.new(0, 0, 0, 20)
			Text.AutomaticSize = Enum.AutomaticSize.X
			Text.Font = Enum.Font.GothamBold
			Text.Text = part.Text
			Text.TextColor3 = part.Color
			Text.TextSize = 12
			Text.TextXAlignment = Enum.TextXAlignment.Left
		end
	end

	return textas
end

